{
  "name": "react-setup",
  "version": "0.0.1",
  "description": "boilerplate for all things in react with webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "analyze:p": "npm run build && webpack-bundle-analyzer ./stats.json",
    "analyze:d": "NODE_ENV=development webpack --config webpack.dev.js && webpack-bundle-analyzer ./stats.json",
    "start": "NODE_ENV=development webpack-dev-server  --config webpack.dev.js"
  },
  "keywords": [
    "react",
    "webpack",
    "redux",
    "boilerplate"
  ],
  "author": "Ravi Sharma",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-ramda": "^1.6.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "style-loader": "^0.22.1",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "ramda": "^0.25.0"
  }
}
